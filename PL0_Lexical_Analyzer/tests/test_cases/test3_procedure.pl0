{test3_procedure.pl0 - Procedure Test}
{Test: Procedure declaration, procedure call, parameter passing}

program proctest;
var x, y, z;

procedure multiply;
begin
    z := x * y
end;

procedure add;
var temp;
begin
    temp := x + y;
    write(temp)
end;

begin
    read(x, y);
    call multiply;
    write(z);
    call add
end